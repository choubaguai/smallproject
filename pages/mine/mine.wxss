page {
  background-color: #eee;
}

.userinfo {
  display: flex;
  flex-direction: column;
  align-items: center;
  background-color: white;
  margin: 20rpx;
  border-radius: 10rpx;
}

.userinfo-avatar {
  width: 128rpx;
  height: 128rpx;
  margin: 20rpx;
  border-radius: 50%;
}

.userinfo-nickname {
  margin-bottom: 20rpx;
  color: #aaa;
}

.userinfo-setting {
  display: flex;
  flex-direction: column;
  background-color: white;
  margin: 20rpx;
  border-radius: 10rpx;
}

.view-tenant-name{
  position: relative;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  padding: 20rpx 20rpx 20rpx 20rpx;
  overflow: hidden;
}
.view-tenant-number{
  position: relative;
  display: flex;
  flex-direction: row;
  margin-left: 20rpx;
  padding: 20rpx 20rpx 20rpx 0rpx;
  overflow: hidden;
  justify-content: space-between;
  border-top: 2rpx solid #e5e5e5;
}
.view-tenant-remark{
  border-top: 2rpx solid #e5e5e5;
}
.text-color{
  color: #808080;
}